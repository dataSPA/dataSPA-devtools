{
  "name": "dataSPA-devtools",
  "description": "A devtools extension for dataSPA. May also work with datastar.",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare"
  },
  "devDependencies": {
    "@internationalized/date": "^3.8.1",
    "@lucide/svelte": "^0.542.0",
    "@tailwindcss/vite": "^4.0.0",
    "@tsconfig/svelte": "^5.0.4",
    "@wxt-dev/module-svelte": "^2.0.3",
    "bits-ui": "^2.8.6",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.6",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.0.0",
    "tslib": "^2.8.1",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.8.3",
    "wxt": "^0.20.6"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "svelte-splitpanes": "^8.0.9",
    "tailwind-merge": "^3.3.1"
  }
}
